  },
  "style": "v2",
  "sitemapLocation": "sitemap.json",
  "usingComponents": {
    "l-button": "/miniprogram_npm/lin-ui/button/index",
    "l-dialog": "/miniprogram_npm/lin-ui/dialog/index",
    "l-toast": "/miniprogram_npm/lin-ui/toast/index",
    "l-input": "/miniprogram_npm/lin-ui/input/index",
    "l-tab-bar": "/miniprogram_npm/lin-ui/tab-bar/index",
    "l-grid": "/miniprogram_npm/lin-ui/grid/index",
    "l-grid-item": "/miniprogram_npm/lin-ui/grid-item/index",
    "l-card": "/miniprogram_npm/lin-ui/card/index",
    "l-popup": "/miniprogram_npm/lin-ui/popup/index",
    "l-list": "/miniprogram_npm/lin-ui/list/index",
    "l-status-show": "/miniprogram_npm/lin-ui/status-show/index",
    "l-message": "/miniprogram_npm/lin-ui/message/index",
    "l-search-bar": "/miniprogram_npm/lin-ui/search-bar/index",
    "l-icon": "/miniprogram_npm/lin-ui/icon/index",
